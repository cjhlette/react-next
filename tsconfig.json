{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": [
      "dom", "es2015", "es2016"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@pages/*": ["./src/pages/*"],
      "@static/*": ["./src/static/*"],
      "@styles/*": ["./src/styles/*"],
      "@lib/*": ["./src/lib/*"],
      "@material-ui/core": ["./material-ui/src"],
      "@material-ui/core/*": ["./material-ui/src/*"],
      "@material-ui/lab": ["./material-ui-lab/src"],
      "@material-ui/lab/*": ["./material-ui-lab/src/*"]
    },
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "next.config.js",
    "server.js",
    "**/.next/**/*.*",
    "**/polyfills.js"
  ]
}
